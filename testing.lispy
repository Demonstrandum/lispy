(define macro (ten-times sym body) (do
  (let (sym 0))
  (iterate (do
    (mutate (sym (+ (eval sym) 1)))
    (if (> (eval sym) 10) break)
    (eval body)))
  (delete sym)))


(ten-times counter (do
  (puts counter)))
